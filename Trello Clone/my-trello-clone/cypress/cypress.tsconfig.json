{
  "compilerOptions": {
    "target": "es2020",
    "lib": ["dom", "esnext"],
    "types": ["cypress", "node"],
    "module": "es2020",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["cypress/**/*.ts"]
}
